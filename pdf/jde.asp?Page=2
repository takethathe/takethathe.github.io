<SCRIPT type=text/javascript >
	str="此处《龙藏》  册  页上/下部 列 字为“  ”，但《永乐北藏》  册  页上/下部 列 字为“  ”，应是印刷问题，联系上下文“  ”，此处用“  ”，同“  ”";
	document.getElementById("reason").value = str;
	
	var div = document.getElementById("reasondiv");
	fjedit=;
	if(fjedit == 1){	
		div.style.display = "block";
	} else {
		div.style.display = "none";
	}

	var tn=0;
	var tli=document.getElementById("hui").getElementsByTagName("ul");
	rollText_tt=setInterval("rollText(tn)",100);
	function rollText(n){
		clearInterval(rollText_tt);
		tn++;
		if (tn>=tli.length){tn=0;}
		rollText_tt=setInterval("rollText(tn)",5000);
		for (var j=0; j<tli.length; j++){
		tli[j].style.display=j==n?"block":"none";
		}
	}

	function edit(id){
		//修改内容
			$.ajax({
				type:"Post",
				url:"CommentLib-jd.asp?action=editcomments&ID="+id.substring(1),
				dataType:"html",
				error: function(){
					alert('Error:修改失败!');
				},
	　　		success:function(result){
					window.location.reload();
		　　 	}
	　　	});
	}

	function noedit(id){
		var $reason = $('#reason').val();//这个就是用户输入的textarea的内容(jquery)
		reason = $reason;
		if(reason ==str){
			alert('Error:校对说明未修改!');
		} else {
			//不修改内容
			reason="<br><span style='font-weight:bold;'>校对说明：</span>"+$reason;
			$.ajax({
				type:"Post",
				url:"CommentLib-jd.asp?action=nocomments&ID="+id.substring(1),
				data:"str="+escape($reason),
				dataType:"html",
				error: function(){
					alert('Error:修改失败!');
				},
	　　		success:function(result){
					window.location.reload();
		　　 	}
	　　	});
		}
	}

	function del(id){
		//删除内容
		if(confirm("您确定要删除吗？")) {
			$.ajax({
				type:"Post",
				url:"CommentLib-jd.asp?action=del&ID="+id.substring(1),
				dataType:"html",
				error: function(){
					alert('Error:删除失败!');
				},
	　　		success:function(result){
					window.location.reload();
		　　 	}
	　　	});
		}else {
			return
		}
	}
</SCRIPT>
